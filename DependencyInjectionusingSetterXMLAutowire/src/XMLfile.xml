<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="student" class="in.spring.beans.Student" scope="prototype" autowire="byName" >
		<property name="sid" value="10" />
		<property name="sname" value="sachin" />
		<property name="saddress" value="IND" />
		
	</bean>
	
	<bean id="work" class="in.spring.beans.Work">
		<property name="play" value="Football" />
		<property name="subject" value="Math" />
	
	
	</bean>

</beans>