<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="student" class="in.spring.beans.Student" scope="prototype" autowire="byName">
		<constructor-arg value="23"/>
		<constructor-arg value="aryan"/>
		<constructor-arg value="mumbai"/>
	</bean>
	
	<bean id="work" class="in.spring.beans.Work">
		<constructor-arg value="football"/>
		<constructor-arg value="Math"/>
	
	
	</bean>

</beans>